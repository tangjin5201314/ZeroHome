#import "NSArray+Helper.h"

@implementation NSArray (Helper)

- (id)safeGetIndexObj:(NSUInteger)index
{
    if (index >= self.count) {
        return nil;
    }
    else{
        return [self objectAtIndex:index];
    }
}
@end
